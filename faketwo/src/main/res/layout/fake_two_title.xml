<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fake_two_title_rel"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:background="#FFf8f8f8"
    >
    <ImageView
        android:id="@+id/fake_two_title_img_finish"
        tools:src="@drawable/fake_two_finish_black"
        android:layout_centerVertical="true"
        android:paddingLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/fake_two_title_txt_other"
        android:layout_toRightOf="@id/fake_two_title_img_finish"
        android:layout_toLeftOf="@id/fake_two_title_txt_title"
        android:layout_centerVertical="true"
        tools:text="@string/fake_two_house"
        android:maxLines="1"
        android:layout_marginRight="15dp"
        android:textColor="@color/fakeTwoColorBlack"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/fake_two_title_txt_title"
        tools:text="首页"
        android:layout_centerInParent="true"
        android:textColor="@color/fakeTwoColorBlack"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <View
        android:id="@+id/fake_two_title_view"
        android:background="#FFDCDCDC"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"/>
</RelativeLayout>